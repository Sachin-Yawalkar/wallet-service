{
  "name": "wallet-service",
  "version": "1.0.0",
  "description": "Backend assessment",
  "main": "dist/index.js",
  "type": "module",
  "imports": {
    "#src/*": "./src/*"
  },
  "scripts": {
    "build": "tsc",
    "init-tables": "tsx src/scripts/initTables.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "dev": "tsx src/index.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "setup": "npm run docker:up && sleep 5 && npm run init-tables",
    "full-test": "npm run setup && npm test"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/ui": "^4.0.16",
    "pino-pretty": "^13.1.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "outDir": "./dist",
  "rootDir": "./src",
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ],
  "strict": true,
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.958.0",
    "@aws-sdk/lib-dynamodb": "^3.958.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/type-provider-typebox": "^6.1.0",
    "fastify": "^5.6.2",
    "ioredis": "^5.8.2"
  }
}
